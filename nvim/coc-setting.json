{
  "languageserver": {
    "go": {
      "command": "gopls",
      "rootPatterns": ["go.mod"],
      "trace.server": "verbose",
      "filetypes": ["go"]
    }
  },
  "explorer.keyMappings.global": {
        "<tab>": "actionMenu"
  },
  "explorer.position": "right",
  "explorer.width": 30,
  "explorer.icon.enableNerdfont": true
}
